        <spring-cloud-starter-netflix-turbine.version>1.4.4.RELEASE</spring-cloud-starter-netflix-turbine.version>
        <spring-cloud-starter-stream-rabbit.version>2.0.0.RELEASE</spring-cloud-starter-stream-rabbit.version>
        <spring-cloud-starter-oauth2.version>1.2.2.RELEASE</spring-cloud-starter-oauth2.version>
        <spring-cloud-starter-netflix-eureka-server.version>2.0.0.RELEASE</spring-cloud-starter-netflix-eureka-server.version>
        <spring-cloud-starter-consul-discovery.version>1.3.3.RELEASE</spring-cloud-starter-consul-discovery.version>
