        <framework-security.version>0.0.1-SNAPSHOT</framework-security.version>
        <framework-trace.version>0.0.1-SNAPSHOT</framework-trace.version>
        <framework-config.version>0.0.1-SNAPSHOT</framework-config.version>
        <framework-task-executor.version>0.0.1-SNAPSHOT</framework-task-executor.version>

        <swagger-butler-core.version>0.0.1-SNAPSHOT</swagger-butler-core.version>
